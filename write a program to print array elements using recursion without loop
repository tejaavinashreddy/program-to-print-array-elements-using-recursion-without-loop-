import java.util.*;
class array
{
public void elements(int i,int a[])
{
if(i==a.length)   
return;
else
{
System.out.print(a[i]+" ");
elements(i+1,a);
}
}
}
class Solution
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int b[]=new int[n];
for(int i=0;i<n;i++)
b[i]=s.nextInt();
array x=new array();
x.elements(0,b);
}
}

